{
    "type": "file",
    "multiprocess": true,
    "pattern": {
        "full": "^((?:[^ ]+ ){3})([^ ]+) ([^ ]+)\\[\\d+\\]: (.*)$",
        "time": "%b %d %H:%M:%S",
        "groups": {
            "time": 0,
            "host": 1,
            "process": 2,
            "message": 3
        }
    }
}
